#include <bits/stdc++.h>
using namespace std;
#define endl "\n"
#define ll long long
#define pb push_back
#define MOD 1000000007
int main()
{
	ll t = 1;
	cin >> t;
	while (t--) {
		ll n, k, x;
		cin >> n >> k >> x;
		if (x > k) {
			cout << -1 << endl;
			continue;
		}
		for (int i = 0; i < n; ++i)
		{
			cout << i % x << " ";
		}
		cout << endl;
	}
	return 0;
}
